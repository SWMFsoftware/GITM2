pro c_a_to_month, timearray, month

  mon = [ $
        'January', $
        'February', $
        'March', $
        'April', $
        'May', $
        'June', $
        'July', $
        'August', $
        'September', $
        'October', $
        'November', $
        'December']

  month = mon(timearray(1)-1)

END

